             <script>
                let num1 = 20
                let num2 = 10
                console.log(num1 + num2)
                console.log(num1 - num2)
                console.log(num1 * num2)
                console.log(num1 / num2)

                var score = 85;
                function calculatorGrade(score) {
                    if (score > 90) {
                        console.log("A grade");
                    } else if (score > 80 & score < 89) {
                        console.log("B grade");
                    } else if (score > 70 & score < 79) {
                        console.log("C grade");
                    } else if (score > 60 & score < 69) {
                        console.log("D grade");
                    } else if (score <60) {
                        console.log("F grade");
                    }
                }
                calculatorGrade(score);
             </script>
             
